{
  "manifest_version": 2,
  "name": "Dump Trump",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["app.js"]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["style.css"]
    }
  ]
}
